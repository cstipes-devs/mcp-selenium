{
  "name": "@angiejones/mcp-selenium",
  "version": "0.1.21",
  "description": "Selenium WebDriver MCP Server",
  "type": "module",
  "main": "src/lib/server.js",
  "bin": {
    "mcp-selenium": "./src/lib/server.js"
  },
  "scripts": {
    "test": "node --test tests/*.test.js",
    "test:watch": "node --test --watch tests/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=html node --test tests/*.test.js",
    "test:lambda": "node test-lambda.js",
    "lambda:test": "LOCAL_TEST=true PORT=3000 node src/lib/lambda.js",
    "lambda:build": "zip -r mcp-selenium-lambda.zip src/ node_modules/ package.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "selenium-webdriver": "^4.18.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "c8": "^10.1.3"
  }
}
